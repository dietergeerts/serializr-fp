{
  "name": "serializr-fp",
  "version": "0.1.2",
  "description": "(De)serialize complex object graphs to/from JSON",
  "main": "dist/serializr-fp.js",
  "module": "src/index.js",
  "files": [
    "dist",
    "src/**/*.js",
    "!src/**/*.test.js",
    "test"
  ],
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack",
    "test": "cross-env NODE_ENV=test nyc --check-coverage mocha src/**/*.test.js",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dietergeerts/serializr-fp.git"
  },
  "keywords": [
    "serialize",
    "deserialize",
    "json",
    "fp"
  ],
  "author": "Dieter Geerts <dieter@dworks.be> (https://dietergeerts.github.io/)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dietergeerts/serializr-fp/issues"
  },
  "homepage": "https://github.com/dietergeerts/serializr-fp#readme",
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-babel": "^1.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "mocha": "^5.0.0",
    "must": "^0.13.4",
    "nyc": "^11.4.1",
    "rc": "^1.2.5",
    "rimraf": "^2.6.2",
    "webpack": "^3.11.0"
  }
}
